conf t
hostname S1

enable secret class 
no ip domain-lookup 

interface Vlan1   
 ip address 192.168.1.1 255.255.255.0 
 no shutdown

banner motd ^C!!!!!!! Unauthorized access to this device is prohibited !!!!!!!^C 
                                                                                 
int range e0/0 - 3
   shutdown
int range e1/1 - 3
  shutdown 
  
int e1/0
 description  Link to PC-A
 switchport access vlan 10
 switchport mode access  
 

Vlan 99
name Management

Vlan10
name Staff  

line con 0            
 password cisco  
 login
 logging synchronous

line vty 0 4  
 password cisco 
 login

end

copy running-config startup-config 
